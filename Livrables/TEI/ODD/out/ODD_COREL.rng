   </define>
   <define name="text">
      <element name="text">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(text) contains a single text of any kind, whether unitary or composite, for example a poem or drama, a collection of essays, a novel, a dictionary, or a corpus sample. [4. Default Text Structure 15.1. Varieties of Composite Text]</a:documentation>
         <group>
            <zeroOrMore>
               <ref name="model.global"/>
            </zeroOrMore>
            <optional>
               <group>
                  <ref name="front"/>
                  <zeroOrMore>
                     <ref name="model.global"/>
                  </zeroOrMore>
               </group>
            </optional>
            <choice>
               <ref name="body"/>
            </choice>
            <zeroOrMore>
               <ref name="model.global"/>
            </zeroOrMore>
         </group>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <ref name="att.global.responsibility.attribute.cert"/>
         <ref name="att.global.responsibility.attribute.resp"/>
         <ref name="att.global.source.attribute.source"/>
         <ref name="att.declaring.attributes"/>
         <ref name="att.written.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="body">
      <element name="body">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(text body) contains the whole body of a single unitary text, excluding any front or back matter. [4. Default Text Structure]</a:documentation>
         <group>
            <zeroOrMore>
               <ref name="model.global"/>
            </zeroOrMore>
            <optional>
               <group>
                  <ref name="model.divTop"/>
                  <zeroOrMore>
                     <choice>
                        <ref name="model.global"/>
                        <ref name="model.divTop"/>
                     </choice>
                  </zeroOrMore>
               </group>
            </optional>
            <optional>
               <group>
                  <ref name="model.divGenLike"/>
                  <zeroOrMore>
                     <choice>
                        <ref name="model.global"/>
                        <ref name="model.divGenLike"/>
                     </choice>
                  </zeroOrMore>
               </group>
            </optional>
            <choice>
               <oneOrMore>
                  <group>
                     <ref name="model.divLike"/>
                     <zeroOrMore>
                        <choice>
                           <ref name="model.global"/>
                           <ref name="model.divGenLike"/>
                        </choice>
                     </zeroOrMore>
                  </group>
               </oneOrMore>
               <oneOrMore>
                  <group>
                     <ref name="model.div1Like"/>
                     <zeroOrMore>
                        <choice>
                           <ref name="model.global"/>
                           <ref name="model.divGenLike"/>
                        </choice>
                     </zeroOrMore>
                  </group>
               </oneOrMore>
               <group>
                  <oneOrMore>
                     <group>
                        <choice>
                           <ref name="model.common"/>
                        </choice>
                        <zeroOrMore>
                           <ref name="model.global"/>
                        </zeroOrMore>
                     </group>
                  </oneOrMore>
                  <optional>
                     <choice>
                        <oneOrMore>
                           <group>
                              <ref name="model.divLike"/>
                              <zeroOrMore>
                                 <choice>
                                    <ref name="model.global"/>
                                    <ref name="model.divGenLike"/>
                                 </choice>
                              </zeroOrMore>
                           </group>
                        </oneOrMore>
                        <oneOrMore>
                           <group>
                              <ref name="model.div1Like"/>
                              <zeroOrMore>
                                 <choice>
                                    <ref name="model.global"/>
                                    <ref name="model.divGenLike"/>
                                 </choice>
                              </zeroOrMore>
                           </group>
                        </oneOrMore>
                     </choice>
                  </optional>
               </group>
            </choice>
            <zeroOrMore>
               <group>
                  <ref name="model.divBottom"/>
                  <zeroOrMore>
                     <ref name="model.global"/>
                  </zeroOrMore>
               </group>
            </zeroOrMore>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-body-sequence-constraint-rule-12">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:body">
               <s:assert test="count(tei:div[@type='chapter'])=7"> Il doit y avoir 7 chapitres
                    dans le document. </s:assert>
            </s:rule>
         </pattern>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <ref name="att.global.responsibility.attribute.cert"/>
         <ref name="att.global.responsibility.attribute.resp"/>
         <ref name="att.global.source.attribute.source"/>
         <ref name="att.declaring.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="div">
      <element name="div">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(text division) contains a subdivision of the front, body, or back of a text. [4.1. Divisions of the Body]</a:documentation>
         <group>
            <zeroOrMore>
               <choice>
                  <ref name="model.divTop"/>
                  <ref name="model.global"/>
               </choice>
            </zeroOrMore>
            <optional>
               <group>
                  <choice>
                     <oneOrMore>
                        <group>
                           <choice>
                              <ref name="model.divLike"/>
                              <ref name="model.divGenLike"/>
                           </choice>
                           <zeroOrMore>
                              <ref name="model.global"/>
                           </zeroOrMore>
                        </group>
                     </oneOrMore>
                     <group>
                        <oneOrMore>
                           <group>
                              <choice>
                                 <ref name="model.common"/>
                              </choice>
                              <zeroOrMore>
                                 <ref name="model.global"/>
                              </zeroOrMore>
                           </group>
                        </oneOrMore>
                        <zeroOrMore>
                           <group>
                              <choice>
                                 <ref name="model.divLike"/>
                                 <ref name="model.divGenLike"/>
                              </choice>
                              <zeroOrMore>
                                 <ref name="model.global"/>
                              </zeroOrMore>
                           </group>
                        </zeroOrMore>
                     </group>
                  </choice>
                  <zeroOrMore>
                     <group>
                        <ref name="model.divBottom"/>
                        <zeroOrMore>
                           <ref name="model.global"/>
                        </zeroOrMore>
                     </group>
                  </zeroOrMore>
               </group>
            </optional>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-div-pb-constraint-rule-13">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                    context="tei:div[@type='chapter']">
               <s:assert test="tei:pb" role="WARN"> L'élément div doit contenir un élément pb
                  </s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-div-sequence-constraint-rule-14">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                    context="tei:div[@type='chapter']">
               <s:assert test="tei:div[@type='section']"> La div 'chapter' doit contenir une div
                    'section' </s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-div-title-constraint-rule-15">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                    context="tei:div[@type='chapter']">
               <s:assert test="tei:head"> La div 'chapter' doit contenir un titre </s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-div-sequence-constraint-rule-16">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                    context="tei:div[@type='section']">
               <s:assert test="tei:div[@type='statute']"> La div 'section' doit contenir une div
                    'statute' </s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-div-title-constraint-rule-17">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                    context="tei:div[@type='section']">
               <s:assert test="tei:head"> La div 'section' doit contenir un titre </s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-div-notBeforeStatute-constraint-rule-18">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                    context="tei:div[@type='statute']">
               <s:assert test="@notBefore"> L'attribut notBefore est obligatoire </s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-div-notAfterStatute-constraint-rule-19">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                    context="tei:div[@type='statute']">
               <s:assert test="@notAfter"> L'attribut notAfter est obligatoire </s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-div-notBeforeSubstatute-constraint-rule-20">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                    context="tei:div[@type='substatute']">
               <s:assert test="@notBefore"> L'attribut notBefore est obligatoire </s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-div-notAfterSubstatute-constraint-rule-21">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                    context="tei:div[@type='substatute']">
               <s:assert test="@notAfter"> L'attribut notAfter est obligatoire </s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-div-idStatute-constraint-rule-22">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                    context="tei:div[@type='statute']">
               <s:assert test="@xml:id"> L'attribut xml:id est obligatoire </s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="ODD_COREL-div-title-constraint-rule-23">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                    context="tei:div[@type='statute']">
               <s:assert test="tei:head"> La div 'statute' doit contenir un titre </s:assert>
            </s:rule>
